# Google Sheets API Configuration
# Copy this file to .env and fill in your actual values

# Google Spreadsheet URL (REQUIRED)
# The URL of the Google Spreadsheet to use for storing registration data
# Example: https://docs.google.com/spreadsheets/d/1AbCdEfGhIjKlMnOpQrStUvWxYz0123456789/edit#gid=0
# You can also just paste the spreadsheet ID directly
GOOGLE_SPREADSHEET_URL=https://docs.google.com/spreadsheets/d/YOUR_SPREADSHEET_ID/edit

# Google Service Account Credentials (REQUIRED)
# Paste the content of your Google Service Account JSON file here.
# Example: {"type":"service_account","project_id":"...","private_key_id":"...","private_key":"...","client_email":"...","client_id":"...","auth_uri":"...","token_uri":"...","auth_provider_x509_cert_url":"...","client_x509_cert_url":"..."}
GOOGLE_SHEETS_CREDENTIALS={"type":"service_account","project_id":"...","private_key_id":"...","private_key":"...","client_email":"...","client_id":"...","auth_uri":"...","token_uri":"...","auth_provider_x509_cert_url":"...","client_x509_cert_url":"..."}